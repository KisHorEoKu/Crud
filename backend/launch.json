{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch NestJS",
        "skipFiles": ["<node_internals>/**"],
        "runtimeArgs": [
          "--nolazy",
          "-r",
          "ts-node/register"
        ],
        "args": ["src/main.ts"],
        "cwd": "${workspaceFolder}",
        "sourceMaps": true,
        "outFiles": ["${workspaceFolder}/dist/**/*.js"]
      }
    ]
  }
  